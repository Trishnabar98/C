#include <stdio.h>

int main(void) {
	// your code goes here
	int number,i,first=0,second=1,third;
	printf("Enter the number:\n");
	scanf("%d",&number);
	printf("Fibonacci Series: ");

	for(i=1;i<=number;++i)
	{
	    printf("%d, ",first);
	    third=first+second;
	    first=second;
	    second=third;
	}
	return 0;
}

