#include <stdio.h>

int main(void) {
	// your code goes here
	int number,sum=0,i;
	printf("Enter the number:\n");
	scanf("%d",&number);
	for(i=1;i<number;i++)
	{
	    if(number %i==0)
	    {
	        sum+=i;
	    }
	}
	if(number==sum)
	    printf("PERFECT_NUMBER\n");
	else
	    printf("NOT PERFECT_NUMBER\n");
	return 0;
}

