#include <stdio.h>

int main(void) {
	// your code goes here
		int number,i,flag=0;
	printf("Enter the Number:\n");
	scanf("%d",&number);
	for(i=2;i<=(number/2);i++)
	{   
	    if(number%i==0)
	    { 
	        flag=1;
	         break;
	    }
	}
	if(number==1)
	    printf("THE NUMBER IS NEITHER PRIME NOR COMPOSITE\n");
	
	 else if(flag)
        	    printf("PRIME\n");
        	else
        	    printf("COMPOSITE\n");
	return 0;
}

