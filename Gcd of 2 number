#include <stdio.h>

int main(void) {
	// your code goes here
	int a,b,gcd,i;
	printf("Enter the 1st number:\n");
	scanf("%d",&a);
	printf("Enter the 2nd number:\n");
	scanf("%d",&b);
	for(i=1;i<=a && i<=b;i++)
	{
	    if(a%i==0 && b%i==0)
	        gcd=i;
	}
	printf("The Gcd of %d & %d is %d\n",a,b,gcd);
	
	return 0;
}

