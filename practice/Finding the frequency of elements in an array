#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	map<int,int> mp;
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++)
	{
	    cin>>a[i];
	}
	for(int i=0;i<n;i++)
	{
	    mp[a[i]]++;
	}
	for(auto it=mp.begin();it!=mp.end();it++)
	{
	    cout<<it->first<<" is occure "<<it->second<<"times"<<endl;
	}
	return 0;
}
