#include <stdio.h>

int main(void) {
	// your code goes here
	int n,i,sum=0;
	printf("enter the value of n:\n");
	scanf("%d",&n);
	for(i=1;i<n;i++)
    {
        if(n%i==0)
        {
         sum+=i;
         
        }
    }
	 if(n<sum)
	    printf("Abundant Number\n");
	else
	    printf("Not Abundant\n");
	return 0;
}

